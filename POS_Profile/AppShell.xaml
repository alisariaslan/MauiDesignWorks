<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="POS_Profile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:mct="clr-namespace:CommunityToolkit.Maui.Behaviors;assembly=CommunityToolkit.Maui"
    xmlns:local="clr-namespace:POS_Profile"
	xmlns:views="clr-namespace:POS_Profile.Views"
    Shell.FlyoutBehavior="Disabled"
	Shell.NavBarIsVisible="False"
	Shell.NavBarHasShadow="False">

	<!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->

	<FlyoutItem>
		<ShellContent Title="Shop" Icon="shop.png" ContentTemplate="{DataTemplate views:Shop}"/>
		<ShellContent Title="Explore" Icon="explore.png" ContentTemplate="{DataTemplate views:Explore}"/>
		<ShellContent Title="Brands" Icon="brands.png" ContentTemplate="{DataTemplate views:Brands}"/>
		<ShellContent Title="Profile" Icon="profile.png" ContentTemplate="{DataTemplate views:Profile}"/>

	</FlyoutItem>

</Shell>
